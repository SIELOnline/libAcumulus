{
    "name": "siel/acumulus",
    "description": "Siel Acumulus Web API library, common to all web shop plugins",
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "php": "^8",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Siel\\Acumulus\\": "src/",
            "Siel\\Acumulus\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siel\\Acumulus\\TestWebShop\\": "tests/TestWebShop"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
